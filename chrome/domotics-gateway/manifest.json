{
  "name": "A-Touch Gateway",
  "short_name": "Gateway",
  "version": "0.9",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "description": "A-Touch software on Gateway module",

  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },

  "icons": {
    "16": "assets/icon-16.png",
    "128": "assets/icon.png"
  },

  "permissions": [
    {
      "socket": [
        "tcp-connect:*:1470"
      ]
    },
    "storage"
  ]
}

