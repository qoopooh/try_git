{
  "name": "AIA Link",
  "version": "0.2",
  "manifest_version": 2,

  "description": "Check online status of AIA Open-Ended Mutual Funds",
  "icons": { "16": "aia16.png",
            "48": "aia48.png",
            "128": "aia128.png" },
  "browser_action": {
    "default_icon": "aia_01.jpg",
    "default_popup": "window.html"
  },

  "permissions": [
    "tabs",
    "https://portal.aia.co.th/",
    "https://direct.aia.co.th/"
  ],
}

