{
  "name": "Tcp Client",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "description": "Show off tcp client functionality.",

  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },

  "icons": {
    "16": "assets/icon-16x16.png",
    "128": "assets/Box-Down-128.png"
  },

  "permissions": [
    {
      "socket": [
        "tcp-connect:*:1470"
      ]
    }
  ]
}

